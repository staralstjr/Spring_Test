<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.staralstjr.spring.lesson02.dao.StoreDAO">

	<!-- Store 데이터 모두 가져오기 쿼리 -->
	<select id="selectStoreList" resultType="com.staralstjr.spring.lesson02.model.Store">
		SELECT
			`id`
			, `name`
			, `phoneNumber`
			, `address`
			, `businessNumber`
			, `introduce`
			, `createdAt`
			, `updatedAt`
		FROM
			`store`
	</select>

</mapper>